{
  "name": "elm-express-boilerplate",
  "version": "0.1.0",
  "engines": {
    "node": "^8.2.1"
  },
  "author": {
    "name": "Paul Duguet",
    "email": "opauldo@gmail.com",
    "url": "https://github.com/OzTK"
  },
  "repository": {
    "type": "Github",
    "url": "https://github.com/OzTK"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all clean ts wp-prod",
    "build:dev": "npm-run-all clean ts wp",
    "build:hot": "npm-run-all clean ts",
    "postbuild": "./postbuild.sh",
    "postbuild:dev": "./postbuild.sh --dev",
    "postbuild:hot": "./postbuild.sh --dev",
    "build:elm": "elm-make --yes",
    "build:views": "elm-view-engine -v server/views -o dist/views",
    "deploy-heroku-postbuild": "elm-package install --yes && npm-run-all build build:views",
    "wp": "webpack --progress",
    "wp-prod": "webpack -p --env.production --progress",
    "ts": "tsc -p .",
    "start": "cd dist && node ./www",
    "start:prod": "NODE_ENV=production node ./www",
    "start:debug": "HOT=true npm run start",
    "watch": "npm-watch build:views",
    "test": "elm-test",
    "snyk:t": "snyk test",
    "snyk:m": "snyk monitor",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "watch": {
    "build:views": {
      "patterns": [
        "server/views",
        "client/elm/**/*"
      ],
      "extensions": "elm",
      "quiet": false
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "config": "^1.26.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "elm-view-engine": "^2.1.2",
    "express": "^4.16.2",
    "hbs": "~4.0.4",
    "hbs-utils": "0.0.4",
    "helmet": "^3.8.2",
    "inversify": "^4.3.0",
    "inversify-express-utils": "^4.0.1",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.10",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winston-express-middleware": "^0.1.0",
    "snyk": "^1.90.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/config": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/elm": "^0.12.29",
    "@types/express": "^4.0.37",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/helmet": "0.0.36",
    "@types/morgan": "^1.7.33",
    "@types/serve-favicon": "^2.2.29",
    "@types/webpack": "^3.0.13",
    "@types/webpack-dev-middleware": "^1.9.3",
    "@types/webpack-hot-middleware": "^2.16.0",
    "@types/winston": "^2.3.6",
    "assets-webpack-plugin": "^3.5.1",
    "clean-webpack-plugin": "^0.1.17",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "elm-css": "^0.6.1",
    "elm-css-webpack-loader": "^3.3.0",
    "elm-hot-loader": "^0.5.4",
    "elm-test": "^0.18.9",
    "elm-webpack-loader": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-sources": "^1.0.1"
  },
  "snyk": true
}
