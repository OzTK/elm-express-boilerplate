  "private": false,
  "scripts": {
    "install": "elm-package install --yes",
    "build": "npm-run-all ts wp-prod",
    "dev": "npm-run-all ts wp",
    "hot": "npm-run-all ts wp-hot",
    "client": "npm-run-all wp",
    "wp": "webpack --progress",
    "wp-prod": "webpack -p --env.production --progress",
    "wp-hot": "webpack --progress --env.hot",
    "ts": "tsc -p .",
    "start": "node ./bin/www",
    "snyk": "snyk test",
    "elm-test": "elm-test",
    "test": "npm-run-all snyk elm-test"
  },
